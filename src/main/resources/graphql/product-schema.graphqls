input ProductRequestDto {
    id: String
    name: String
    image: Date
    quantity: Double
    price: Double
    discount: Double
    page: Int!
    size: Int!

    #Search
    listFields: [String]
    listSorted: [Map]
    fromBirthday: String
    toBirthday: String
    createFromDate: String
    createToDate: String
    isActive: Boolean
}

type ProductResponseDto {
    id: String
    name: String
    image: Date
    quantity: Double!
    price: Double!
    discount: Double!
    created: Date
    updated: Date
}

type CommonPageInfoProduct {
    total: Int!
    page: Int!
    size: Int!
    data: [ProductResponseDto]
}